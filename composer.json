{
    "name": "niels/lemondedupc",
    "description": "List of dependency for the Le Monde Du PC Project",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "google/recaptcha": "^1.2",
        "helios-ag/fm-elfinder-bundle": "^10.0",
        "knplabs/knp-paginator-bundle": "^5.2",
        "scheb/2fa-backup-code": "^5.8",
        "scheb/2fa-bundle": "^5.8",
        "scheb/2fa-google-authenticator": "^5.8",
        "scheb/2fa-qr-code": "^5.8",
        "sensio/framework-extra-bundle": "^5.5",
        "suncat/mobile-detect-bundle": "dev-master",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.2.*",
        "symfony/cache": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/security-csrf": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.19",
        "symfony/stopwatch": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "classmap-authoritative": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
