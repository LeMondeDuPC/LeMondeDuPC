# Articles, cat√©gories, pagination, commentaires et recherche
RewriteRule ^article/([0-9]+)/(.*)$ articles/article.php?article=$1 [L]
RewriteRule ^categorie/(.*)/page/([0-9]+)$ articles/categorie.php?categorie=$1&page=$2 [L]
RewriteRule ^categorie/(.*)$ articles/categorie.php?categorie=$1 [L]
RewriteRule ^recherche/(.*)/page/([0-9]+)$ articles/resultat.php?q=$1&page=$2 [L]
RewriteRule ^recherche/(.*)$ articles/resultat.php?q=$1 [L]
RewriteRule ^page/([0-9]+)$ index.php?page=$1 [L]

# Espace membre
RewriteRule ^membre/inscription$ espace-membre/inscription.php [L]
RewriteRule ^membre/connexion$ espace-membre/connexion.php [L]
RewriteRule ^membre/([0-9]+)/profil$ espace-membre/profil.php?id=$1 [L]
RewriteRule ^membre/([0-9]+)/deconnexion$ espace-membre/deconnexion.php?id=$1 [L]
RewriteRule ^membre/([0-9]+)/edition/(.*)$ espace-membre/editionprofil.php?id=$1&token=$2 [L]
RewriteRule ^membre/([0-9]+)/parametre/(.*)$ espace-membre/parametre.php?id=$1&token=$2 [L]
RewriteRule ^membre/confirmer/(.*)/(.*)$ espace-membre/confirmation.php?pseudo=$1&key=$2 [L]

# Newsletter
RewriteRule ^newsletter/(.*)/(.*)/(.*)$ divers/newsletter.php?id=$2&key=$3&action=$1 [L]

